# Vercel Blob Storage
BLOB_READ_WRITE_TOKEN=your_blob_token_here

# Vercel KV Database
# These will be automatically set by Vercel when you create a KV database
# For local development, you can leave these empty and the app will use fallback data
KV_URL=
KV_REST_API_URL=
KV_REST_API_TOKEN=
KV_REST_API_READ_ONLY_TOKEN=

# JWT Authentication
JWT_SECRET=your_jwt_secret_here

# Stripe (if you're using payments)
STRIPE_SECRET_KEY=your_stripe_secret_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# Add this token from your Vercel dashboard:
# 1. Go to your Vercel project dashboard
# 2. Go to Settings > Environment Variables  
# 3. Add BLOB_READ_WRITE_TOKEN with the token from your Vercel Blob store
